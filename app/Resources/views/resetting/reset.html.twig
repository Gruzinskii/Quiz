{% trans_default_domain "translations" %}

{% block stylesheets %}
    <link rel="stylesheet" href="{{ asset("static/styles/resetting/reset.css?v1.1") }}">
    <link rel="stylesheet" href="{{ asset("static/styles/bootstrap.min.css") }}">
    <link rel="icon" type="image/x-icon" href="{{ asset('QUIZ.png') }}" />
{% endblock %}

{% block body %}
    <div class="col-4 user-password bg-color accommodation">
        <div class="container text">
            <h2>Reset password</h2>
            {{ form_start(form) }}
            {{ form_label(form.email) }}
            {{ form_widget(form.email) }}
            {{ form_errors(form)}}
            {{ form_end(form) }}

        </div>
    </div>

{% endblock %}